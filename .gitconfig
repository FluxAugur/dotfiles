[user]
	name = Nathanial L. McConnell
	email = fluxaugur@gmail.com
[alias]
	b = branch
	ch = log --cherry --pretty=format:\"%C(auto)%h (%ar) %an: %s\" --date-order --reverse
	ci = commit
	cm = commit -m
	co = checkout
	df = diff
	find = log --pretty=\"format:%Cgreen%H %Cblue%s\" --name-status --grep
	g = grep -I
	lg = log -p
	loq  = log --graph --all --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(bold white)â€” %an%C(reset)%C(bold yellow)%d%C(reset)' --abbrev-commit --date=relative
	st = status
[branch]
	autosetuprebase = always
[color]
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow
	frag = magenta
	old = red
	new = green
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[commit]
	gpgsign = true
	template = $HOME/.gitcommit.template
[core]
	pager = delta
[delta]
	features = side-by-side line-numbers decorations
	whitespace-error-style = 22 reverse
[delta "decorations"]
	commit-decoration-style = bold yellow box ul
	file-style = bold yellow ul
	file-decoration-style = none
[github]
	user = FluxAugur
[init]
	defaultBranch = main
[interactive]
	diffFilter = delta --color-only
[protocol]
	version = 2
[pull]
	rebase = false
[push]
	default = simple
[rerere]
	enabled = 1
